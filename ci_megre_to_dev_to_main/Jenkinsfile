pipeline {
    agent any

    stages {
        stage("Merge to Main") {
            steps {
                script {
                    sh 'git -c credential.helper=store --credentials=${82fecaa5-889b-4d66-b0ae-f3f0c998438c	} remote set-url origin git@github.com:Norbert00/rectangle_area_calculator_CI_CD_process.git'
                    sh 'git checkout main'
                    sh 'git merge origin/dev'
                    sh 'git push origin main'
                }
            }
        }
    }
}

